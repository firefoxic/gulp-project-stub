{
  "private": true,
  "type": "module",
  "browserslist": [
    "last 2 version",
    "not ie <=11",
    "not dead",
    "not op_mini all",
    "not < 0.25%"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "babel": {
    "ignore": [
      "node_modules/**/*"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "browser-sync": "^2.26.12",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^9.0.0",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-svgstore": "^7.0.1",
    "path": "^0.12.7",
    "postcss-csso": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "standard": "^14.3.4",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "start": "gulp --debug",
    "build": "gulp buildProject",
    "prod": "NODE_ENV=production gulp buildProject",
